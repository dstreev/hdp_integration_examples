<bundle-app name='bundle-one' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xmlns='uri:oozie:bundle:0.2'>
    <coordinator name='coord-one' >
        <app-path>${coordPathOne}</app-path>
        <configuration>
            <property>
                <name>startOne</name>
                <value>${startOne}</value>
            </property>
            <property>
                <name>endOne</name>
                <value>${endOne}</value>
            </property>
            <property>
                <name>whichWorkflow</name>
                <value>one</value>
            </property>
            <property>
                <name>baseDir</name>
                <value>${baseDir}/coord-one</value>
            </property>
            <property>
                <name>subDirOne</name>
                <value>${baseDir}/coord-one/one</value>
            </property>
            <property>
                <name>frequency</name>
                <value>${frequencyOne}</value>
            </property>
            <property>
                <name>start</name>
                <value>${startOne}</value>
            </property>
            <property>
                <name>end</name>
                <value>${endOne}</value>
            </property>
        </configuration>
    </coordinator>


    <coordinator name='coord-two' >
        <app-path>${coordPathTwo}</app-path>
        <configuration>
            <property>
                <name>start</name>
                <value>${startTwo}</value>
            </property>
            <property>
                <name>end</name>
                <value>${endTwo}</value>
            </property>
            <property>
                <name>whichWorkflow</name>
                <value>two</value>
            </property>
            <property>
                <name>baseDir</name>
                <value>${baseDir}/coord-two</value>
            </property>
            <property>
                <name>subDirTwo</name>
                <value>${baseDir}/coord-two/two</value>
            </property>
            <property>
                <name>frequency</name>
                <value>${frequencyTwo}</value>
            </property>
            <property>
                <name>start</name>
                <value>${startTwo}</value>
            </property>
            <property>
                <name>end</name>
                <value>${endTwo}</value>
            </property>
        </configuration>
    </coordinator>
</bundle-app>
